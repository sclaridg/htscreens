	month = nov,
	year = {2018},
	keywords = {cancer, CRISPR, functional genomics, interferon gamma pathway, mass cytometry, pooled screen, protein barcodes, single cell analysis, T cells, tumor immunology},
	pages = {1141--1155.e16},
