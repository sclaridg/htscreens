	urldate = {2018-05-04},
	journal = {Oncotarget},
	author = {Siegel, Matthew B. and Liu, Selina Qiuying and Davare, Monika A. and Spurgeon, Stephen E. and Loriaux, Marc M. and Druker, Brian J. and Scott, Emma C. and Tyner, Jeffrey W.},
	month = may,
	year = {2015},
	pmid = {26254279},
	pmcid = {PMC4662464},
	pages = {18921--18932},
	file = {PubMed Central Full Text PDF:/Users/claris01/Zotero/storage/JW5GQB7Q/Siegel et al. - 2015 - Small molecule inhibitor screen identifies synergi.pdf:application/pdf}
}

