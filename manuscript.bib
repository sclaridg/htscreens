	author = {Hatzis, Christos and Bedard, Philippe L. and Juul Birkbak, Nicolai and Beck, Andrew H. and Aerts, Hugo J. W. L. and Stern, David F. and Shi, Leming and Clarke, Robert and Quackenbush, John and Haibe-Kains, Benjamin},
