	pages = {7--34},
	file = {Siegel et al. - 2019 - Cancer statistics, 2019 Cancer Statistics, 2019.pdf:/Users/claris01/Zotero/storage/HBAEWX3R/Siegel et al. - 2019 - Cancer statistics, 2019 Cancer Statistics, 2019.pdf:application/pdf}
}

@article{an_met_2014,
