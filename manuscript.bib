	month = may,
	year = {2019},
	keywords = {machine learning, adenylate energy charge, antibiotics, ATP, biochemical screen, LC-MS/MS, metabolism, NADPH:NADP ratio, network modeling, purine biosynthesis},
	pages = {1649--1661.e9},
