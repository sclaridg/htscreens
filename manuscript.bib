  author = {Papillon-Cavanagh, Simon and De Jay, Nicolas and Hachem, Nehme and Olsen, Catharina and Bontempi, Gianluca and Aerts, Hugo J W L and Quackenbush, John and Haibe-Kains, Benjamin},
  file = {/Users/claris01/Zotero/storage/9FAVHK9U/Papillon-Cavanagh et al. - 2013 - Comparison and validation of genomic predictors fo.pdf},
  eprinttype = {pmid},
  eprint = {23355484},
