	pages = {1011--1021},
	file = {PubMed Central Full Text PDF:/Users/claris01/Zotero/storage/YECRBMYK/Simon - 2014 - Stratification and Partial Ascertainment of Biomar.pdf:application/pdf}
}

@inproceedings{laifenfeld_early_2012,
