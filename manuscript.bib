	month = mar,
	year = {2015},
	pmid = {25432174},
	pages = {880--891},
	file = {Full Text PDF:/Users/claris01/Zotero/storage/DPJIAY6E/Javidi-Sharifi et al. - 2015 - Crosstalk between KIT and FGFR3 Promotes Gastroint.pdf:application/pdf;Snapshot:/Users/claris01/Zotero/storage/YWNJUA2G/880.html:text/html}
}

@article{bicocca_crosstalk_2012,
	title = {Crosstalk between {ROR}1 and the {Pre}-{B} {Cell} {Receptor} {Promotes} {Survival} of t(1;19) {Acute} {Lymphoblastic} {Leukemia}},
