	month = feb,
	year = {2008},
	pmid = {18025156},
	pages = {2238--2245},
	file = {Full Text PDF:/Users/claris01/Zotero/storage/ZF4N7Q5I/Tyner et al. - 2008 - RNAi screening of the tyrosine kinome identifies t.pdf:application/pdf;Snapshot:/Users/claris01/Zotero/storage/GW8CHAW5/2238.html:text/html}
}

