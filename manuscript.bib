  editor = {Goryanin, Igor I. and Goryachev, Andrew B.},
  options = {useprefix=true},
  file = {/Users/claris01/Zotero/storage/758AEBHI/Laifenfeld et al. - 2012 - Early Patient Stratification and Predictive Biomar.pdf}
