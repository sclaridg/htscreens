	pmcid = {PMC3721163},
	pages = {597--602},
	file = {PubMed Central Full Text PDF:/Users/claris01/Zotero/storage/9FAVHK9U/Papillon-Cavanagh et al. - 2013 - Comparison and validation of genomic predictors fo.pdf:application/pdf}
