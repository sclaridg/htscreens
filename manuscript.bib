  author = {Sharifnia, Tanaz and Rusu, Victor and Piccioni, Federica and Bagul, Mukta and Imielinski, Marcin and Cherniack, Andrew D. and Pedamallu, Chandra Sekhar and Wong, Bang and Wilson, Frederick H. and Garraway, Levi A. and Altshuler, David and Golub, Todd R. and Root, David E. and Subramanian, Aravind and Meyerson, Matthew},
