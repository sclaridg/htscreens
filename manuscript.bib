	pages = {675--682},
	file = {Full Text PDF:/Users/claris01/Zotero/storage/3GWEP4ZJ/An et al. - 2014 - MET amplification is not rare and predicts unfavor.pdf:application/pdf;Snapshot:/Users/claris01/Zotero/storage/EKCDAFAL/cncr.html:text/html}
}

@article{kim_prognostic_2018,
