	file = {Mouradov et al. - 2014 - Colorectal Cancer Cell Lines Are Representative Mo.pdf:/Users/claris01/Zotero/storage/QXVI6GVL/Mouradov et al. - 2014 - Colorectal Cancer Cell Lines Are Representative Mo.pdf:application/pdf}
}

