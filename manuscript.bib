  pages = {181ra50-181ra50},
  author = {Cheng, Wei-Yi and Yang, Tai-Hsien Ou and Anastassiou, Dimitris},
  file = {/Users/claris01/Zotero/storage/4SJSIR5T/Cheng et al. - 2013 - Development of a Prognostic Model for Breast Cance.pdf;/Users/claris01/Zotero/storage/528AIW9B/181ra50.html},
