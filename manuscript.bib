  file = {/Users/claris01/Zotero/storage/4RBD9GCT/nature11003-s1.pdf;/Users/claris01/Zotero/storage/74I5Q5LJ/Barretina et al. - 2012 - The Cancer Cell Line Encyclopedia enables predicti.pdf;/Users/claris01/Zotero/storage/XYJ2DRN9/nature11003-s2.pdf;/Users/claris01/Zotero/storage/CXTBQPBQ/nature11003.html}
