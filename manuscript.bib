  pages = {513},
  author = {Cohen, Adam L and Soldi, Raffaella and Zhang, Haiyu and Gustafson, Adam M and Wilcox, Ryan and Welm, Bryan E and Chang, Jeffrey T and Johnson, Evan and Spira, Avrum and Jeffrey, Stefanie S and Bild, Andrea H},
  file = {/Users/claris01/Zotero/storage/K5ZCK9Y9/Cohen et al. - 2011 - A pharmacogenomic method for individualized predic.pdf},
  eprinttype = {pmid},
  eprint = {21772261},
  pmcid = {PMC3159972}
}

