	pmcid = {PMC5391733},
	pages = {513--519},
	file = {PubMed Central Full Text PDF:/Users/claris01/Zotero/storage/83C5L2DL/Huang et al. - 2017 - The cancer precision medicine knowledge base for s.pdf:application/pdf}
