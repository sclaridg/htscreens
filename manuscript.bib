	journal = {New England Journal of Medicine},
	author = {Slamon, Dennis J. and Leyland-Jones, Brian and Shak, Steven and Fuchs, Hank and Paton, Virginia and Bajamonde, Alex and Fleming, Thomas and Eiermann, Wolfgang and Wolter, Janet and Pegram, Mark and Baselga, Jose and Norton, Larry},
